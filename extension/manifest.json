{
  "manifest_version": 2,
  "name": "Clipboard Sync",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Clipboard Sync"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "notifications",
    "storage"
  ]
}