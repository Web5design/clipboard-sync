{
  "manifest_version": 2,
  "name": "Clipboard Sync",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Clipboard Sync",
    "default_icon": "icon19.png"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "notifications",
    "storage"
  ],
  "web_accessible_resources": [
    "icon32-downloading.png",
    "icon32-error.png"
  ]
}